#Requires AutoHotkey v2.0

class Resolve {
    class Panel {

        static Bin := "{F1}"
        static Keyframes := "{F2}"

        static Properties := "{F3}"
        static Effects := "{F4}"
        static Player := '{F7}'
        static NextPanel := "^+{$}"
        static PrevPanel := "^+{^}"
    }
    
    static MousePointer := "{<}"

    static SearchMenu := "^+{space}"
    static SearchFx := "^{space}"
}